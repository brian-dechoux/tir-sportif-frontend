{
  "extends": [
    "tslint-react"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "prettier": true,
    "interface-name": false,
    "jsx-boolean-value": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "semicolon": true
  }
}
